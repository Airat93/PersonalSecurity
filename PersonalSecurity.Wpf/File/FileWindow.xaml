<Window x:Name="Window" x:Class="PersonalSecurity.Wpf.File.FileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FileWindow" Height="217" Width="353">
    <Grid x:Name="Grid">
        <Label x:Name="FileType" Content="Тип файла" HorizontalAlignment="Left" Margin="67,43,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="FileTypeText" HorizontalAlignment="Left" Height="23" Margin="67,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158"/>
        <Button x:Name="LoadFile" Content="Загрузить файл" HorizontalAlignment="Left" Margin="33,128,0,0" VerticalAlignment="Top" Width="129" Height="36" Click="LoadFile_Click"/>
        <Button x:Name="Exit" Content="Выйти" HorizontalAlignment="Left" Margin="248,155,0,0" VerticalAlignment="Top" Width="75" Click="Exit_Click"/>

    </Grid>
</Window>
